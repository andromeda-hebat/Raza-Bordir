{
    "name": "andromeda/sistem-informasi-umkm-raza-bordir",
    "description": "Sistem informasi UMKM Raza Bordir adalah sebuah sistem yang membantu UMKM dengan fokus pada penjualan di bidang bordir",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/config/config.php"
        ]
    },
    "require": {
        "vlucas/phpdotenv": "^5.6"
    },
    "scripts": {
        "db-schema": "php database/schema.php",
        "db-seeder": "php database/seeder.php",
        "db-init": [
            "@db-schema",
            "@db-seeder"
        ]
    }
}
